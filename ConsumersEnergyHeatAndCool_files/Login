function SubmitButtonForLogin(n,t,i){n.on("submit",function(){window.dataLayer&&window.dataLayer.push({event:"eventTracking",category:"Customer",action:"Login",label:i});startLoadingButton(t)});n.submit()}function HideModalMobile(){$("#MobileLoginModal").addClass("d-none");$("#MobileLoginmodalBackground").addClass("d-none");$("#MobileLoginPage").addClass("d-none");$("#MobileLoginPage").removeClass("isvisible");$(".msp_master-homepage").show();$(".wp-footer").show()}var cookie=objectCookie.getCookie("MobileLoginPage"),viewFullSiteCookie=objectCookie.getCookie("MobileCookieFullSiteView"),orientationFlag;$(function(){$("#passwordLoginBlock").keypress(function(){$("#pagePassword").hasClass("parsley-success")&&$("#pagePassword").removeClass("login-error")});$(".event-prevent-copy").on("copy",function(){return!1});$(".event-prevent-drag").on("drag",function(){return!1});$("#modalLoginRejectButton").click(function(){createCookie("MobileLoginPage","true",1e3);HideModalMobile()});$("#FullSiteButtonId").click(function(){cookie!==""||$("#MobileLoginPage").hasClass("d-none")||(viewFullSiteCookie===""?($("#MobileLoginModal").removeClass("d-none"),$("#MobileLoginmodalBackground").removeClass("d-none")):HideModalMobile())});var t=window.location.hash,n=$("#login-form");n.attr("action",n.attr("action")+t)});$(document).ready(function(){cookie!==""||$("#MobileLoginPage").hasClass("d-none")||viewFullSiteCookie!==""&&HideModalMobile();$(".wp_server-error-message").length===1&&window.dataLayer&&setTimeout(dataLayer.push({event:"eventTracking",category:"Customer",action:"Login",label:"Failed"}),1e4);$(".comp-login .keeplogged .lookup-tooltip").click(function(n){n.preventDefault()})});$("#modalLoginAceptButton").click(function(){objectCookie.removeCookie("MobileCookieFullSiteView");HideModalMobile()});$(".homebuttonfooter").on("click",function(n){n.preventDefault();objectCookie.removeCookie("MobileLoginPage");document.location.href=$(".homebuttonfooter").data("url-mobile-homepage")});orientationFlag=!1;$(window).on("orientationchange",function(){orientationFlag=!orientationFlag;orientationFlag===!1&&cookie===""&&$("#MobileLoginPage").slideDown();$(window).width()>991&&$("#MobileLoginPage").slideUp()})